{
  "name": "inversearena-backend",
  "version": "0.1.0",
  "description": "InverseArena payout execution server",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test:request-validation": "tsx --test tests/request-validation.unit.test.ts",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@stellar/stellar-sdk": "^13.1.0",
    "@types/pg": "^8.16.0",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0",
    "pg": "^8.12.0",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.33",
    "@types/pino": "^7.0.4",
    "@types/pino-http": "^5.8.4",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.14.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5"
  }
}
